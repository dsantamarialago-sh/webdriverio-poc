{
  "name": "webdriverio-poc",
  "version": "1.0.0",
  "description": "WebdriverIO proof of concept",
  "main": "index.js",
  "scripts": {
    "test": "clear && npx wdio wdio.conf.js && npm run allure",
    "test:multi": "npx wdio wdio.multibrowser.conf.js",
    "test:ie11": "clear && npx wdio wdio.ie11.conf.js",
    "test:firefox": "clear && npx wdio wdio.firefox.conf.js",
    "test:safari": "clear && npx wdio wdio.safari.conf.js",
    "test:chrome": "clear && npx wdio wdio.chrome.conf.js",
    "allure": "allure generate ./reports/allure/allure-results --clean && allure open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^6.1.14",
    "@wdio/cli": "^6.10.5",
    "@wdio/local-runner": "^6.10.5",
    "@wdio/mocha-framework": "^6.10.4",
    "@wdio/selenium-standalone-service": "^6.10.4",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.10.4",
    "allure-commandline": "^2.13.0",
    "properties-reader": "^2.1.1",
    "wdio-json-reporter": "^2.0.0"
  }
}
